						{
							"type": "script",
							"language": "javascript",
							"script": [
								"var puterScript = document.createElement(\"script\");",
								"puterScript.src = \"https://js.puter.com/v2/\";",
								"window.document.head.append(puterScript);",
								"puterScript.onload = getGameData;",
								"",
								"async function getGameData() {",
								"    const response = await puter.kv.get('gameData');",
								"    if (response) {",
								"        runtime.callFunction(\"GetPuterData\", response);",
								"    }",
								"    else {",
								"        runtime.callFunction(\"SetPuterDefaultData\");",
								"    }",
								"}"
